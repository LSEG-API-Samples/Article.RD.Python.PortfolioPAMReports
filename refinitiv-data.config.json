{
  "http": {        
     "request-timeout": 120
  },
  "logs": {
    "level": "debug",
    "transports": {
      "console": {
        "enabled": false
      },
      "file": {
        "enabled": false,
        "name": "/tmp/refinitiv-data-lib.log"
      }
    }
  },
  "sessions": {
    "default": "platform.dp",
    "desktop": {
      "codebook": {
        "app-key": "<appkey>"
      }
    },
    "platform": {
        "dp": {
            "app-key": "<appkey>",
            "username": "<user>",
            "password": "<password>",
            "signon_control": true
        }
    }
  }
}